<core:FragmentDefinition
	xmlns="sap.ui.comp.valuehelpdialog"
	xmlns:m="sap.m"
	xmlns:fb="sap.ui.comp.filterbar"
	xmlns:core="sap.ui.core">
	<ValueHelpDialog
		title="Product"
		ok=".onHkontValueHelpOkPress"
		cancel=".onHkontValueHelpCancelPress"
		afterClose=".onHkontValueHelpAfterClose"
		supportRanges="true"
		key="SAKNR"
		descriptionKey="MCOD1">
		<filterBar>
			<fb:FilterBar advancedMode="true" search=".onFilterBarSearch" isRunningInValueHelpDialog="true">
				<fb:filterGroupItems>
					<fb:FilterGroupItem
						groupName="__$INTERNAL$"
						name="SAKNR"
						label="GL Account"
						visibleInFilterBar="true">
						<fb:control>
							<m:Input name="SAKNR" />
						</fb:control>
					</fb:FilterGroupItem>
					<fb:FilterGroupItem
						groupName="__$INTERNAL$"
						name="MCOD1"
						label="Description"
						visibleInFilterBar="true">
						<fb:control>
							<m:Input name="MCOD1" />
						</fb:control>
					</fb:FilterGroupItem>
				</fb:filterGroupItems>
			</fb:FilterBar>
		</filterBar>
	</ValueHelpDialog>
</core:FragmentDefinition>
